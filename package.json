{
  "name": "boardytale_proxy_server",
  "version": "0.0.2",
  "scripts": {
    "start-dev": "npm run set-config-dev && dart runner/run.dart",
    "set-config-dev": "ts-node set-environment.ts --config=dev-config.ts",
    "ts2dart": "ts2dart",
    "generate-shared": "cd shared && pub run build_runner build && prettier --write \"**/*.ts\"",
    "remove-duplicated-files": "remove-duplicated-files",
    "start-proxy": "nodemon --watch proxy_server --exec ts-node proxy_server/index.ts",
    "start-user-server": "dart user_server/bin/main.dart",
    "ts-node": "ts-node",
    "editor-serve": "cd editor_client && dart serve.dart"
  },
  "private": true,
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/node": "~8.9.4",
    "@types/prettier": "^1.13.2",
    "@types/request": "^2.47.1",
    "@types/set-cookie-parser": "0.0.2",
    "child_process": "^1.0.2",
    "express": "^4.16.3",
    "gzipper": "^1.4.1",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "ts-node": "~5.0.1",
    "tslint": "^5.11.0",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "http-proxy-middleware": "^0.19.0"
  }
}
